modules = ["nodejs-20", "web", "python-3.11", "bash"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["firefox-esr", "fluxbox", "git", "openbox", "tigervnc", "wget"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VNC Desktop Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Main VNC Project"

[[workflows.workflow]]
name = "VNC Desktop Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 start_vnc.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Main VNC Project"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 run.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
